      {{!log 'ruleset partial' this}}
      <div class="table_wrapper">
      <table>
          <tr>
          <th>Ruleset</th>
            <th>Rule</th>
            <th>Attribute</th>
            <th>Operator</th>
            <th>Value</th>
          </tr>
          {{#each associatedRuleSets}}
          <tr>
            {{!-- calaculate rowspan based on length of ruleSetDefinition array --}}
          <td rowspan="{{ruleSetDefinition.length}}">
              {{name}} ({{code}})<br>
              <b>Start Date:</b> {{formatDate ruleSetDefinitionStartDate}}<br>
              <b>End Date:</b> {{formatDate ruleSetDefinitionEndDate}}<br>
          </td>
            {{#each ruleSetDefinition}}
              <td>{{name}} ({{code}})</td>
              <td>{{ruleAttribute.name}} ({{ruleAttribute.code}})</td>
              <td>{{operator}}</td>
              <td>{{values}}</td>
            </tr>
            {{/each}}
        </tr>
      {{/each}}
      </table>
      </div>
